	"@gif#474946383961" & _
	"@riff#52494646" & _
	"@mp3#494433" & _
	"@mp3#FFFB" & _
	"class#CAFEBABE" & _
	"@mkv#1A45DFA3" & _
	"@txt#"
sWordDocument = "W" & Chr(0) & "o" & Chr(0) & "r" & Chr(0) & "d" & Chr(0) & "D" & Chr(0) & "o" & Chr(0) & "c" & Chr(0) & "u" & Chr(0) & "m" & Chr(0) & "e" & Chr(0) & "n" & Chr(0) & "t"
