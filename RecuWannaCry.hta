Dim bescribir
bescribir = "NO"
bActulizarConsola = False
iEsperaCierre = 0
bCerrar = False
sVersion = Mid(window.document.documentElement.innerHTML,InStr(window.document.documentElement.innerHTML,"version")+8,8)
